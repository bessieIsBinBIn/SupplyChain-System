{"remainingRequest":"/home/guotiezheng/vues/vue-store/node_modules/vue-loader/lib/index.js??vue-loader-options!/home/guotiezheng/vues/vue-store/src/views/Orders.vue?vue&type=style&index=0&id=4b6b7b1f&scoped=true&lang=css&","dependencies":[{"path":"/home/guotiezheng/vues/vue-store/src/views/Orders.vue","mtime":1623308116826},{"path":"/home/guotiezheng/vues/vue-store/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/home/guotiezheng/vues/vue-store/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/home/guotiezheng/vues/vue-store/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/home/guotiezheng/vues/vue-store/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/home/guotiezheng/vues/vue-store/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Orders.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4KA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"Orders.vue","sourceRoot":"src/views","sourcesContent":["<!--\n * @Description: 全部商品页面组件(包括全部商品,商品分类,商品搜索)\n * @Author: hai-27\n * @Date: 2020-02-07 16:23:00\n * @LastEditors: hai-27\n * @LastEditTime: 2020-03-08 12:11:13\n -->\n<template>\n  <div class=\"goods\" id=\"goods\" name=\"goods\">\n    <!-- 面包屑 -->\n    <div class=\"breadcrumb\">\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\">\n        <el-breadcrumb-item to=\"/\">首页</el-breadcrumb-item>\n        <el-breadcrumb-item>我的订单</el-breadcrumb-item>\n      </el-breadcrumb>\n    </div>\n    <!-- 面包屑END -->\n\n\n\n    <!-- 主要内容区 -->\n    <div class=\"main\">\n        <div>\n        <el-table :data=\"tableData.slice((currentPage-1)*pagesize,currentPage*pagesize)\" style=\"width: 100%\" highlight-current-row=true >\n      <el-table-column prop=\"ID\" label=\"订单ID\" >\n      </el-table-column>\n      <el-table-column prop=\"seller\" label=\"供货商\" >\n      </el-table-column>\n      <el-table-column prop=\"goodsname\" label=\"货品名称\">\n      </el-table-column>\n      <el-table-column prop=\"deadline\" label=\"截止时间\">\n      </el-table-column>\n      <el-table-column prop=\"status\" label=\"订单状态\">\n      </el-table-column>\n      <el-table-column  >\n        <template slot-scope=\"scope\">\n        <el-button  @click=\"handleNewOrder(scope.row)\">详细信息</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n          <el-pagination background layout=\"prev, pager, next\" :page-size=\"pagesize\" :total=\"this.total\" @current-change=\"current_change\" class=\"page\">\n\t\t</el-pagination>\n      </div>\n    </div>\n    <el-dialog title=\"订单详情\"\n      class=\"detail-dialog\"\n      :visible.sync=\"showDetailDialog\"\n      :close-on-click-modal=\"false\"\n      :close-on-press-escape=\"false\">\n      <el-form class=\"detail-form\" :model=\"detailForm\">\n        <el-form-item label=\"订单ID：\" prop=\"ID\" label-width=\"100px\">\n          <span>{{detailForm.ID}}</span>\n        </el-form-item>\n        <el-form-item label=\"订单状态：\" prop=\"status\" label-width=\"100px\">\n          <span>{{detailForm.status}}</span>\n        </el-form-item>\n        <el-form-item label=\"物流状态：\" prop=\"transferstatus\" label-width=\"100px\">\n          <span>{{detailForm.transferstatus}}</span>\n        </el-form-item>\n        <el-form-item label=\"物流ID：\" prop=\"transferID\" label-width=\"100px\">\n          <span>{{detailForm.transferID}}</span>\n        <el-form-item label=\"发起企业：\" prop=\"buyer\" label-width=\"100px\">\n          <span>{{detailForm.buyer}}</span>\n        </el-form-item>\n        <el-form-item label=\"供货商：\" prop=\"seller\" label-width=\"100px\">\n          <span>{{detailForm.seller}}</span>\n        </el-form-item>\n        <el-form-item label=\"货品名称：\" prop=\"goodsname\" label-width=\"100px\">\n          <span>{{detailForm.goodsname}}</span>\n        </el-form-item>\n        <el-form-item label=\"货品数量：\" prop=\"goodsnum\" label-width=\"100px\">\n          <span>{{detailForm.goodsnum}}</span>\n        </el-form-item>\n        <el-form-item label=\"收货地址：\" prop=\"address\" label-width=\"100px\">\n          <span>{{detailForm.address}}</span>\n        </el-form-item>\n        <el-form-item label=\"订单总金额：\" prop=\"orderamount\" label-width=\"100px\">\n          <span>{{detailForm.orderamount }}</span>\n        </el-form-item>\n        <el-form-item label=\"创建时间：\" prop=\"createtime\" label-width=\"100px\">\n          <span>{{detailForm.createtime}}</span>\n        </el-form-item>\n        <el-form-item label=\"截止时间：\" prop=\"deadline\" label-width=\"100px\">\n          <span>{{detailForm.deadline}}</span>\n        </el-form-item>\n        <el-form-item label=\"备注：\" prop=\"remark\" label-width=\"100px\">\n          <span>{{detailForm.remark}}</span>\n        </el-form-item>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"\">\n        <el-button @click=\"closeForm()\">关闭</el-button>\n      </div>\n    </el-dialog>\n    <!-- 主要内容区END -->\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      tableData:[{\"goodsID\":'1',\"name\":'钢筋',\"goodtype\":'1',\"amount\":'100吨',\"standards\":'10*100',\"material\":'精钢',\"workmanship\":'冷拉',\"supplier\":'北京钢铁集团',\"adddate\":'2021-6-7',\"tel\":'123123123',\"shippingarea\":'北京市辖区',\"remark\":'无'},\n      {\"goodsID\":'1',\"name\":'钢筋',\"goodtype\":'1',\"amount\":'100吨',\"standards\":'10*100',\"material\":'精钢',\"workmanship\":'冷拉',\"supplier\":'北京钢铁集团',\"adddate\":'2021-6-7',\"tel\":'123123123',\"shippingarea\":'北京市辖区',\"remark\":'无'},\n      {\"goodsID\":'1',\"name\":'钢筋',\"goodtype\":'1',\"amount\":'100吨',\"standards\":'10*100',\"material\":'精钢',\"workmanship\":'冷拉',\"supplier\":'北京钢铁集团',\"adddate\":'2021-6-7',\"tel\":'123123123',\"shippingarea\":'北京市辖区',\"remark\":'无'}],\n      goodsname:'',\n      standards:'',\n      material:'',\n      workmanship:'',\n      showDetailDialog: false,\n      detailForm: {},\n      total:0,\n      currentPage:1,\n      pagesize:15\n    };\n  },\n  created() {\n    this.getData()\n  },\n  activated() {\n  },\n  watch: {},\n  methods: {\n    handleNewOrder(row) {\n        console.log(\"下单啦\")\n        this.detailForm.buyer=''\n        this.detailForm.goodsnum=''\n        this.detailForm.orderamount=''\n        this.detailForm.deadline=''\n        this.detailForm.reamrk=''\n        this.showDetailDialog = true\n        this.detailForm.goodsID = row.goodsID\n        this.detailForm.seller = row.supplier\n        this.goodsname = row.name\n        this.material = row.material\n        this.workmanship = row.workmanship\n        this.standards = row.standards\n        let date = new Date();\n       let year = date.getFullYear(); // 年\n       year = year +  \"年\"\n       let month = date.getMonth() + 1; // 月\n       month = month + \"月\"\n        let day = date.getDate(); // 日\n        day = day + \"日\"\n        let hour = date.getHours(); // 时\n        hour = hour < 10 ? \"0\" + hour : hour; // 如果只有一位，则前面补零\n        let minute = date.getMinutes(); // 分\n        minute = minute < 10 ? \"0\" + minute : minute; // 如果只有一位，则前面补零\n        let time = hour + \":\" + minute\n        this.detailForm.createtime = year + month + day + time\n    },\n     filterHandler(value, row, column) {\n        const property = column['property'];\n        return row[property] === value;\n    },\n    closeForm() {\n      this.showDetailDialog = false\n    },\n      current_change(currentPage){\n      this.currentPage = currentPage;\n      },\n\n    getData() {\n      this.total = this.tableData.length\n    },\n\n\n  }\n};\n</script>\n\n<style scoped>\n.goods {\n  background-color: #f5f5f5;\n}\n/* 面包屑CSS */\n.el-tabs--card .el-tabs__header {\n  border-bottom: none;\n}\n.goods .breadcrumb {\n  height: 50px;\n  background-color: white;\n}\n.goods .breadcrumb .el-breadcrumb {\n  width: 1225px;\n  line-height: 30px;\n  font-size: 16px;\n  margin: 0 auto;\n}\n/* 面包屑CSS END */\n\n\n/* 主要内容区CSS */\n.goods .main {\n  margin: 0 auto;\n  max-width: 1225px;\n}\n.goods .main .list {\n  min-height: 650px;\n  padding-top: 14.5px;\n  margin-left: -13.7px;\n  overflow: auto;\n}\n\n.goods .main .none-product {\n  color: #333;\n  margin-left: 13.7px;\n}\n\n    .page {\n    margin-top: 20px;\n    text-align: center\n  }\n/* 主要内容区CSS END */\n</style>"]}]}